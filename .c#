using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            const double pi = 3.14;
            
            
            
            double r;
            double a; 
    
            r=Convert.ToDouble(Console.ReadLine()); 
            a=pi*r*r;   
            Console.WriteLine(a); 
            Console.ReadLine();          
            
            
        }
    }
}
